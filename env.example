# LuckyWalk 환경 변수 예시
# 실제 사용 시 .env 파일로 복사하고 값들을 설정하세요

# Supabase 설정
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Mobile Ads (나중에 활성화)
ADMOB_APP_ID_IOS=your_ios_admob_app_id
ADMOB_APP_ID_ANDROID=your_android_admob_app_id
ADMOB_REWARDED_AD_UNIT_ID_IOS=your_ios_rewarded_ad_unit_id
ADMOB_REWARDED_AD_UNIT_ID_ANDROID=your_android_rewarded_ad_unit_id

# Firebase (나중에 활성화)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_APP_ID_IOS=your_ios_firebase_app_id
FIREBASE_APP_ID_ANDROID=your_android_firebase_app_id

# Apple 로그인
APPLE_CLIENT_ID=your_apple_client_id
APPLE_TEAM_ID=your_apple_team_id
APPLE_KEY_ID=your_apple_key_id
APPLE_PRIVATE_KEY=your_apple_private_key

# Kakao 로그인
KAKAO_NATIVE_APP_KEY=your_kakao_native_app_key
KAKAO_JAVASCRIPT_APP_KEY=your_kakao_javascript_app_key
KAKAO_REST_API_KEY=your_kakao_rest_api_key

# 앱 설정
APP_VERSION=1.0.0
MIN_SUPPORTED_VERSION=1.0.0
DEBUG_MODE=true

# 로깅 설정
LOG_LEVEL=info
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true

# 보안 설정
ENCRYPTION_KEY=your_32_character_encryption_key
JWT_SECRET=your_jwt_secret_key

# 복권 설정
DEFAULT_TICKET_PRICE=100
MAX_TICKETS_PER_SUBMISSION=1000
DRAW_TIME_UTC=11:50
SETTLEMENT_TIME_UTC=03:00
